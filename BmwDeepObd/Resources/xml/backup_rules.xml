<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
        <exclude domain="file" path="Ecu"/>
        <exclude domain="file" path="Download"/>
        <include domain="external " path="."/>
        <exclude domain="external" path="Ecu"/>
        <exclude domain="external" path="Download"/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
        <include domain="external " path="."/>
    </device-transfer>
</data-extraction-rules>
